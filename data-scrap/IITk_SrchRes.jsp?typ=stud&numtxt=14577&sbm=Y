		<script language="JavaScript" type="text/javascript">

function frmsbm()
{
	var x;
	x = 0;
	if(IsEmptyString(document.srchfrm.numtxt.value))
	{
		alert("Please enter Pfno/RollNo to search");
		document.srchfrm.numtxt.focus();
		x=x+1;
		return false;
	}
  if(x==0)
  {
    document.srchfrm.method = "post";
		document.srchfrm.action= "IITk_SrchRes.jsp?sbm=Y";
		document.srchfrm.submit();
	}
	return true;
}


function IsEmptyString(str)
{
	nLen = str.length;
	for(i=0; i<nLen; ++i)
	{
		if(str.charAt(i) != " ")
		{
			return false;
		}
	}
	return true;
}

function backfn()
{
if (document.srchfrm.bk.value=="Y")
{
 document.srchfrm.method="post";
 document.srchfrm.action="IITK_Srch.jsp?typ=stud";
 document.srchfrm.submit();
}
else
window.history.go(-1);
}

