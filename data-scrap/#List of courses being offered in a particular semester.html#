<title>List of courses being offered in a particular semester</title>
<script language="JavaScript">
//Given 1,2 or 3 returns first/second/summer
function genUserFriendlySemester(semester)
{
	var sem=new String(semester);
	var answer;
	if (sem.valueOf()=="1")
		answer="I (first)";
	else if (sem.valueOf()=="2")
		answer="II (second)";
	else
		answer="Summer";
	document.write(answer);
}
</script>
<!--opens up a new pop-up window with the specified url-->
<script language="JavaScript">
function popUp(url,windowName)
{
	var new_window
// stop on 08/01/2010	new_window = window.open(url,windowName,"width=550,height=550,toolbar=0,location=0,directories=0,status=0,menubar=0,scrollbar=1,scrollbars=0,resizable=1,copyhistory=0")
	new_window = window.open(url,windowName,"width=550,height=350,toolbar=0,location=0,directories=0,status=0,menubar=0,scrollbar=1,scrollbars=1,resizable=1,copyhistory=0,dependent=1")
	
}
</script>
<script language="JavaScript">
function popUp(url,windowName)
{
	var new_window
	new_window = window.open(url,windowName,"width=550,height=550,toolbar=0,location=0,directories=0,status=0,menubar=0,scrollbar=1,scrollbars=0,resizable=1,copyhistory=0")
}
</script>

